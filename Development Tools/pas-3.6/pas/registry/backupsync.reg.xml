<?xml version="1.0"?>
<registry>
  <rfield name="idbackupsync">
    <rdata type="fieldtype">strFBFieldTypeInt</rdata>
    <rdata type="label">idbackupsync</rdata>
  </rfield>
  <rfield name="actiontime">
    <rdata type="fieldtype">strfFBFieldTypeDate</rdata>
    <rdata type="datef">d/m/Y - H:i:s</rdata>
  </rfield>
  <rfield name="sqlstatement">
    <rdata type="fieldtype">strFBFieldTypeText</rdata>
    <rdata type="label">SQL Statement</rdata>
    <rdata type="default"></rdata>
    <rdata type="textarea">60:30:</rdata>
  </rfield>
  <rfield name="dbname">
    <rdata type="fieldtype">strFBFieldTypeChar</rdata>
    <rdata type="label">Database Name</rdata>
    <rdata type="default"></rdata>
    <rdata type="textline">50:65</rdata>
  </rfield>
</registry>